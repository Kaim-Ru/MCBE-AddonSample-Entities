{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "kaimtemp:movableentity",
      "materials": {
        "default": "entity_alphatest"
      },
      "textures": {
        "default": "textures/entity/blankentity"
      },
      "geometry": {
        "default": "geometry.movableentity"
      },
      "animations": {
        "move": "animation.movableentity.move",
        "look_at_target": "animation.common.look_at_target"
      },
      "scripts": {
        "animate": [{ "move": "q.modified_move_speed" }, "look_at_target"]
      },
      "render_controllers": ["controller.render.default"],
      "spawn_egg": {
        "texture": "movableentity_spawn_egg"
      }
    }
  }
}
